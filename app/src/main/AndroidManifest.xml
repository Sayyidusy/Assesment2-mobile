<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.assesment2.estuff">

    <application
        android:name="com.assesment2.estuff.data.DatabaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.InventoryApp">
        <activity
            android:exported="true"
            android:name="com.assesment2.estuff.view.main.FirstActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.assesment2.estuff.view.history.HistoryActivity"
            android:parentActivityName=".MainActivity"/>

        <activity
            android:name="com.assesment2.estuff.view.main.ArtikelActivity"
            android:parentActivityName=".MainActivity"/>

        <activity
            android:name="com.assesment2.estuff.MainActivity"
            android:exported="false"/>

    </application>

</manifest>